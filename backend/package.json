{
  "name": "compassionate-ai-backend",
  "version": "2.0.0",
  "description": "Research-grade compassionate AI chatbot with cultural adaptation and hierarchy awareness for Indonesian context",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "node test-api.js",
    "analyze": "node analyze-data.js",
    "export-all": "node export-research-data.js",
    "reset-data": "rm -rf research-data && mkdir research-data",
    "backup-data": "cp -r research-data research-data-backup-$(date +%Y%m%d-%H%M%S)"
  },
  "keywords": [
    "ai",
    "chatbot",
    "cultural-adaptation",
    "hierarchy-awareness",
    "compassionate-ai",
    "research",
    "indonesian-context",
    "design-science",
    "empathy"
  ],
  "author": "Aidil - Information Systems Research",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.10.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.2.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/compassionate-ai-research"
  },
  "bugs": {
    "url": "https://github.com/your-username/compassionate-ai-research/issues"
  },
  "research": {
    "purpose": "ISR Special Issue on Compassionate AI",
    "methodology": "Design Science Research",
    "context": "Indonesian Organizational Settings",
    "contribution": "Cultural Adaptation Framework for Compassionate AI"
  }
